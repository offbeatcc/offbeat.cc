{
  "name": "muboard",
  "version": "0.5.2",
  "description": "Self-rendering, interactive, and distributable mathematics chalkboards with support for Markdown and LaTeX",
  "main": "muboard.js",
  "files": [
    "muboard.js",
    "muboard.min.js"
  ],
  "keywords": [
    "markdown",
    "commonmark",
    "tex",
    "latex",
    "browser"
  ],
  "homepage": "https://muboard.net/",
  "bugs": "https://github.com/susam/muboard/issues",
  "license": "MIT",
  "author": "Susam Pal <susam@susam.net> (https://susam.net/)",
  "repository": "github:susam/muboard",
  "devDependencies": {
    "jsdoc": "latest",
    "standard": "latest",
    "uglify-js": "latest"
  },
  "scripts": {
    "test": "standard",
    "min": "uglifyjs muboard.js --compress --mangle --output muboard.min.js",
    "docs": "jsdoc -c jsdoc.json"
  }
}
